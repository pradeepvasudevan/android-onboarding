<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_white"
    tools:context=".ui.OnboardingWebviewActivity">
    <uk.co.santander.onboarding.base.SanWebView
        android:id="@+id/onboarding_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <FrameLayout
        android:id="@+id/app_progress_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/san_red_progress_viewgroup"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true">

        <ProgressBar
            android:id="@+id/app_progress"
            style="@style/SanProgressBarLight"
            android:layout_gravity="center"
            android:indeterminateTint="@color/pri_one"
            android:visibility="gone" />
    </FrameLayout>
</RelativeLayout>